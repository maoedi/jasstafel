{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Generate Settings",
      "type": "shell",
      "command": "flutter",
      "args": ["packages", "pub", "run", "build_runner", "build"],
      "problemMatcher": []
    },
    {
      "label": "Generate Localization",
      "type": "flutter",
      "command": "flutter",
      "args": ["gen-l10n"],
      "problemMatcher": []
    },
    {
      "label": "Generate Icons",
      "type": "flutter",
      "command": "flutter",
      "args": ["pub", "run", "flutter_launcher_icons:main"],
      "problemMatcher": []
    },
    {
      "label": "Start Android emulator",
      "command": "emulator",
      "args": ["-avd", "Pixel_6_API_Sv2"],
      "options": {
        "cwd": "${env:HOME}/Android/Sdk/tools/"
      },
      "problemMatcher": []
    }
  ]
}
